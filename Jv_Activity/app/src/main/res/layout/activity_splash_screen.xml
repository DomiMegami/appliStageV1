<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="dominique.fr.jv_activity.SplashScreenActivity"
    android:orientation="vertical">
    <com.jaredrummler.android.widget.AnimatedSvgView
        android:id="@+id/animated_svg_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_marginBottom="0dp"
        app:animatedSvgFillColors="@array/jv_colors"
        app:animatedSvgGlyphStrings="@array/jv_strings"
        app:animatedSvgFillStart="1000"
        app:animatedSvgFillTime="2000"
        app:animatedSvgImageSizeX="2048"
        app:animatedSvgImageSizeY="2732"
        app:animatedSvgTraceTime="2000"
        app:animatedSvgTraceTimePerGlyph="1000"/>
</LinearLayout>
